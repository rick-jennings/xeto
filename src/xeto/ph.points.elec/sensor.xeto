ElecAcPhaseAggExportApparentPowerSensor : ElecAcExportApparentPowerSensor { elecAcPhaseAgg: ElecAcPhaseAgg }

// Sensor for net of AC electric apparent power aggregated across phases
ElecAcPhaseAggNetApparentPowerSensor : ElecAcNetApparentPowerSensor { elecAcPhaseAgg: ElecAcPhaseAgg }

// Sensor for AC electric power factor
ElecAcPhasePfSensor : ElecAcPfSensor { elecAcPhase: ElecAcPhase }

// Sensor for AC electric power factor aggregated across phases
ElecAcPhaseAggPfSensor : ElecAcPfSensor { elecAcPhaseAgg: ElecAcPhaseAgg }

