ElecAcPhaseImportApparentPowerSensor : ElecAcImportApparentPowerSensor { elecAcPhase: ElecAcPhase }

// Sensor for export of AC electric apparent power
ElecAcPhaseExportApparentPowerSensor : ElecAcExportApparentPowerSensor { elecAcPhase: ElecAcPhase }

// Sensor for net of AC electric apparent power
ElecAcPhaseNetApparentPowerSensor : ElecAcNetApparentPowerSensor { elecAcPhase: ElecAcPhase }

